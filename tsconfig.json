  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "lib": [
      "es2019"
    ],
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "sourceRoot": "/app"
  }
}